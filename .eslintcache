%_.~+]*)*' + // port and path\r\n            '(\\\\?[;&a-z\\\\d%_.~+=-]*)?' + // query string\r\n            '(\\\\#[-a-z\\\\d_]*)?$', 'i'); // fragment locator\r\n        return !!pattern.test(str);\r\n    }\r\n\r\n\r\n    const displayRecipeCard = (recipe, index) => {\r\n        return (\r\n            <div key={index} className='recipeCard'>\r\n                <img className='recipeImage' src={validURL(recipe.imageURL) ? recipe.imageURL : 'https://images.pexels.com/photos/1640777/pexels-photo-1640777.jpeg?auto=compress&cs=tinysrgb&dpr=1&w=500'} alt='ImageHere' />\r\n                <div className='textArea'>\r\n                    <h3 className='recipeTitle'>{recipe.recipeName}</h3>\r\n                    <div className='leftRightContainer'>\r\n                        <div className='left'>\r\n                            <p className='ingredientLists'>{recipe.ingredientsList}</p>\r\n                            <br />\r\n                            <p className='ingredientLists'>{recipe.description}</p>\r\n                            <br />\r\n                            <p className='ingredientLists'>NOTE: Quantity is measured in {recipe.unit}</p>\r\n                        </div>\r\n                        <div className='right'>\r\n                            <div className='row'>\r\n                                <h4 className='cardSubtitle'>Ingredients</h4>\r\n                                <h4 className='cardSubtitle'>Quantity</h4>\r\n                            </div>\r\n                            {\r\n                                recipe.ingredients.map((ingredient, subIndex) => (\r\n                                    <div key={subIndex} className='rowData'>\r\n                                        <p className='cardSubtitleData'>{ingredient.ingredient}</p>\r\n                                        <p className='cardSubtitleData'>{ingredient.quantity}</p>\r\n                                    </div>\r\n                                ))\r\n                            }\r\n                        </div>\r\n                    </div>\r\n                    <br />\r\n                    <br />\r\n                    <p className='footerText'>Added by: {recipe.authorName}</p>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n\r\n    const regex = new RegExp(searchValue, \"gi\");\r\n\r\n    const searchResults = recipeFromStore.filter((recipe) => {\r\n\r\n        if (recipe.recipeName && recipe.recipeName.match(regex)) {\r\n            return true;\r\n        } else {\r\n            let doesIncludes = false;\r\n            recipe.ingredients.forEach(({ ingredient }) => {\r\n                if (ingredient.match(regex)) { doesIncludes = true; }\r\n            })\r\n            if (doesIncludes) {\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n\r\n    });\r\n\r\n\r\n    return (\r\n        <div className='home'>\r\n            <h2 className='title'><span>Burgeon</span> Recipe</h2>\r\n            <div className='field-area'>\r\n                <div>\r\n                    <div className='searchArea'>\r\n                        <input\r\n                            className=\"search\"\r\n                            name=\"search\"\r\n                            type=\"text\"\r\n                            value={searchValue}\r\n                            onChange={(e) =>\r\n                                setSearchValue(e.target.value)\r\n                            }\r\n                            placeholder='Search by ingredients.'\r\n                        />\r\n                    </div>\r\n                </div>\r\n                <button className=\"addMoreButton\">\r\n                    <Link className='link' to=\"/add\">Add More Recipe</Link>\r\n                </button>\r\n                <button className=\"addMoreButton\">\r\n                    <Link className='link' to=\"/json\">JSON Page</Link>\r\n                </button>\r\n            </div>\r\n            {\r\n                recipeFromStore.length > 0 ? <div className='flex-wrap'>\r\n                    {\r\n                        searchResults.length > 0 ? searchResults.map((recipe, index) => (\r\n                            displayRecipeCard(recipe, index))\r\n                        ) :\r\n                            recipeFromStore.map((recipe, index) => (\r\n                                displayRecipeCard(recipe, index)\r\n                            ))\r\n                    }\r\n                </div> : <Error />\r\n            }\r\n        </div>\r\n    );\r\n}\r\n\r\n\r\nexport default Home;","C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Components\\AddRecipe\\AddRecipe.js",[],"C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Components\\Error\\Error.js",[],"C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Redux\\index.js",[],"C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Redux\\Reducer\\Reducer.js",[],"C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Redux\\Action\\Action.js",[],"C:\\Users\\aashi\\Desktop\\Projects\\ReactJS\\recipe\\src\\Components\\JsonPage\\JsonPage.js",["43"],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":22,"column":8,"nodeType":"50","endLine":22,"endColumn":10,"suggestions":"51"},{"ruleId":"48","severity":1,"message":"52","line":14,"column":8,"nodeType":"50","endLine":14,"endColumn":10,"suggestions":"53"},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["56"],"React Hook useEffect has a missing dependency: 'jsonDATA'. Either include it or remove the dependency array.",["57"],"no-global-assign","no-unsafe-negation",{"desc":"58","fix":"59"},{"desc":"60","fix":"61"},"Update the dependencies array to be: [dispatch]",{"r